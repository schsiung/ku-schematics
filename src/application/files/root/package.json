{
    "name": "ku-ng-startup",
    "version": "0.0.1",
    "license": "MIT",
    "description": "ng-zorro-antd admin panel front-end framework",
    "engines": {
        "node": ">= 8.10.0",
        "npm": ">= 3.0.0"
    },
    "private": true,
    "scripts": {
        "precommit": "npm run lint-staged",
        "ng": "ng",
        "start": "ng serve",
        "local": "ng serve -o -c local",
        "serve:hmr": "ng serve -o --hmr -e=hmr",
        "build": "ng build --prod --build-optimizer",
        "analyze": "ng build --prod --build-optimizer --stats-json",
        "lint": "npm run lint:ts && npm run lint:style",
        "lint:ts": "tslint  'src/**/*.ts'",
        "lint:style": "stylelint \"{src}/**/*.less\" --syntax less",
        "lint-staged": "lint-staged",
        "e2e": "ng e2e",
        "test": "ng test",
        "test-coverage": "ng test -sr -cc -sm=false",
        "test:ci": "ng test --browsers Chrome_no_sandbox"
    },
    "dependencies": {
        "@angular/animations": "6.1.7",
        "@angular/common": "6.1.7",
        "@angular/compiler": "6.1.7",
        "@angular/core": "6.1.7",
        "@angular/forms": "6.1.7",
        "@angular/http": "6.1.7",
        "@angular/platform-browser": "6.1.7",
        "@angular/platform-browser-dynamic": "6.1.7",
        "@angular/router": "6.1.7",
        "@angularclass/hmr": "2.1.3",
        "@ku/auth": "0.0.5",
        "@ku/ui": "0.0.7",
        "ajv": "6.4.0",
        "chokidar": "2.0.3",
        "classlist.js": "1.1.20150312",
        "core-js": "2.5.4",
        "file-saver": "1.3.3",
        "jshint": "2.9.6",
        "ng-block-ui": "1.0.6",
        "ng-zorro-antd": "1.7.0",
        "ngx-clipboard": "11.1.5",
        "rxjs": "6.0.0",
        "rxjs-compat": "6.1.0",
        "screenfull": "3.3.1",
        "web-animations-js": "2.3.1",
        "zone.js": "0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.6.8",
        "@angular-devkit/build-ng-packagr": "~0.6.3",
        "@angular-devkit/core": "~0.7.5",
        "@angular/cli": "6.2.6",
        "@angular/compiler-cli": "6.1.7",
        "@angular/language-service": "6.0.2",
        "@types/jasmine": "~2.8.6",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "codelyzer": "~4.2.1",
        "editorconfig-tools": "0.1.1",
        "husky": "0.14.3",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~1.7.1",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~1.4.2",
        "karma-jasmine": "~1.1.1",
        "karma-jasmine-html-reporter": "0.2.2",
        "less": "2.7.0",
        "lint-staged": "7.1.2",
        "ng-alain": "1.0.1",
        "ng-packagr": "4.4.0",
        "prettier": "1.12.1",
        "prettier-stylelint": "0.4.2",
        "protractor": "~5.3.0",
        "stylelint": "9.2.0",
        "stylelint-config-standard": "18.2.0",
        "ts-node": "~5.0.1",
        "tsickle": "0.33.0",
        "tslib": "1.9.0",
        "tslint": "~5.9.1",
        "tslint-config-prettier": "1.12.0",
        "tslint-language-service": "0.9.9",
        "typescript": "2.9.2"
    },
    "lint-staged": {
        "*.{cmd,html,json,md,sh,txt,xml,yml}": [
            "editorconfig-tools fix",
            "git add"
        ],
        "*.ts": [
            "npm run lint:ts",
            "prettier --write",
            "git add"
        ],
        "*.less": [
            "prettier --write",
            "git add"
        ]
    }
}